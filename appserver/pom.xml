                            <jarType>api</jarType>
                            <specVersion>${javax.security.enterprise-spec.version}</specVersion>
                            <specImplVersion>${jakarta.security.enterprise.version}</specImplVersion>
                            <apiPackage>javax.security.enterprise</apiPackage>
