                            <jarType>api</jarType>
                            <specVersion>${jakarta.security.auth.message-api.version}</specVersion>
                            <specImplVersion>${jakarta.security.auth.message-api.version}</specImplVersion>
                            <apiPackage>javax.security.auth.message</apiPackage>
